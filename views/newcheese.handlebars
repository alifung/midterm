<!-- google maps search places found at https://developers.google.com/maps/documentation/javascript/examples/places-placeid-finder -->


<div id="map"></div>
<script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -34.397, lng: 150.644},
          zoom: 8
        });
      }
</script>

  <form action="/cheeses/add" method="post" enctype="multipart/form-data" id ="form">


  <!-- map div stuff -->

  <input id="pac-input" class="controls" type="text"
          placeholder="Enter a location">
      <div id="map"></div>

  <!-- map div stuff -->

  <label for="tag">Categorize Dish: </label>
  <select name="tag" id="tag">
  	<option value="Mac and Cheese">Mac and Cheese</option>
  	<option value="Italian">Italian</option>
  	<option value="Sandwiches">Sandwiches</option>
  </select>

  <label for="image">Image: </label>
  <input id="image" type="file" name="image" accepts="image/*">

  <input type="submit">
  </form>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCxRaWgaD-gARm7vJkVpHrqyJf8qGrNohI&libraries=places&callback=initMap"async defer></script>
